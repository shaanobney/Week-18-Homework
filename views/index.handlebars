<div class="jumbotron">
  <div id="content">
    <a class="articleURL" href="{{ this.articleURL }}">
      <div id="picture">
        <img src="{{ this.imgURL }}">
      </div>
    </a>
    <h3 id="headli">{{ this.title }}</h3>
    <p id="blurble">{{ this.synopsis }}</p>
    <div id="buttz">
      <div id="buttons">
        <button id="next" class="basic" data-id="{{ this._id }}">NEXT</button>
      </div>
    </div>
  </div>

<div id="rapper">
<div id="ding">
  <div class="comments">
    <h3 class="comms">Post A Comment</h3>
    <input id="comment" type="text" name="comment" placeholder="rumble grumble gromble">
    <button id="post" data-id="{{ this._id }}">POST</button>
  </div>
</div>
<div id="dong">
  <div class="comments">
    <h3 class="comms">Earlier Comments</h3>
    <div id="comment-holder">
      {{#each this.comments as |value key|}}
        <p class="trollist"><span class="number">{{ addOne key }}</span> {{ value.text }} <a href="#" class="remove" data-id="{{ value._id }}"><i class="fa fa-trash" aria-hidden="true"></i></a></p>
      {{/each}}
    </div>
  </div>
</div>
</div>
</div>
